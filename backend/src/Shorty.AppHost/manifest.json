{
  "resources": {
    "shorty-cache": {
      "type": "container.v0",
      "connectionString": "{shorty-cache.bindings.tcp.host}:{shorty-cache.bindings.tcp.port}",
      "image": "docker.io/library/redis:7.2",
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 6379
        }
      }
    },
    "shorty-api": {
      "type": "project.v0",
      "path": "../Shorty.API/Shorty.API.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "ConnectionStrings__shorty-cache": "{shorty-cache.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "frontend": {
      "type": "executable.v0",
      "workingDirectory": "../../../frontend",
      "command": "npm",
      "args": [
        "run",
        "start"
      ],
      "env": {
        "NODE_ENV": "development",
        "services__shorty-api__http__0": "{shorty-api.bindings.http.url}",
        "services__shorty-api__https__0": "{shorty-api.bindings.https.url}",
        "PORT": "{frontend.bindings.https.targetPort}"
      },
      "bindings": {
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8000
        }
      }
    }
  }
}