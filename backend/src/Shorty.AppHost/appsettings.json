{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "CreateToken": {
        "ClusterId": "api",
        "Match": {
          "Path": "/create",
          "Methods": [ "POST" ]
        }
      },
      "ResolveToken": {
        "ClusterId": "api",
        "Match": {
          "Path": "{**catch-all}",
          "Methods": [ "GET" ]
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "api": {
            "Address": "http://shorty-api"
          }
        }
      }
    }
  }
}
